     <script src="https://cdn.datatables.net/1.10.12/js/dataTables.bootstrap.min.js"></script>
  </head>
  <body>
  <table id="example"  class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>Project Id</th>  
                <th>Name</th>
                <th>CreatedAt</th>  
                <th>UpdatedAt</th> 
            </tr>
        </thead>
        <tfoot>
            <tr>
                <th>Project Id</th>  
                <th>Name</th>
                <th>CreatedAt</th>  
                <th>UpdatedAt</th> 
            </tr>
        </tfoot>
    </table>
    <script>
        /*$(document).ready(function(){
            $('#example').DataTable({
                "ajax":"http://localhost:5000/static/data/project.json"
            });
        });*/
    $(document).ready(function() {
        var dataTable = $('#example').DataTable( {
            //"processing": true,
            "serverSide": true,
            "ajax":{
                //url :"http://localhost:5000/api/Projects/getpage", // json datasource
                url:"http://localhost:5000/api/Projects/getpage",
                type: "post",  // method  , by default get
                error: function(){  // error handling
                    $(".example-error").html("");
                    $("#example").append('<tbody class="employee-grid-error"><tr><th colspan="3">No data found in the server</th></tr></tbody>');
                    $("#example_processing").css("display","none");
                }
            }
        } );
    } );
    </script>
  </body>
</html>